{
  "name": "webaudio-oscilloscope",
  "version": "4.0.0",
  "keywords": [
    "webaudio",
    "oscilloscope",
    "level meter",
    "audio meter",
    "audio visualizer",
    "web-audio",
    "audio",
    "html5 audio",
    "getusermedia",
    "analyzernode",
    "audiocontext",
    "audio editor",
    "audio recorder",
    "audio analyzer"
  ],
  "main": "dist/index.js",
  "source": "index.js",
  "distDir": "dist",
  "targets": {
    "example": {
      "distDir": "dist-example",
      "source": "example/index.html",
      "publicUrl": "./"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:theanam/webaudio-oscilloscope.git"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "parcel": "^2.0.1"
  },
  "homepage": "https://theanam.github.io/webaudio-oscilloscope/",
  "scripts": {
    "run-example": "parcel --target example --dist-dir temp-example",
    "build-example": "parcel build --target example",
    "build": "parcel build index.js",
    "watch": "parcel watch index.js",
    "publish-page": "yarn build-example && npx gh-pages --dist-dir dist-example && rm -rf dist-example"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
